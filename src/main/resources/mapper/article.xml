<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.com.yourHelper.Mapper.ArticleMapper">
	<select id="findAllListByIndexId" resultType="kr.com.yourHelper.Dto.ArticleDto">
		SELECT *
		FROM article where article_list_id = #{index}
	</select>
